{
    "name": "Les joies du code NewTab",
    "description": "Onglet custom permettant d'afficher un gif aleatoire depuis le site lesjoiesducode.fr",
    "version": "1.0",
    "manifest_version": 2,
    "minimum_chrome_version": "37",
    "icons": {
        "16": "imgs/icon16.png",
        "48": "imgs/icon48.png",
        "128": "imgs/icon128.png"
    },
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "permissions": [
        "http://api.tumblr.com/v2/*",
        "https://api.tumblr.com/v2/*"
    ]
}
